<div id="wrapper">

    <div class="normalheader ">
        <div class="hpanel">
            <div class="panel-body">
                <a class="small-header-action" href="#">
                    <div class="clip-header">
                        <i class="fa fa-arrow-up"></i>
                    </div>
                </a>

                <div id="hbreadcrumb" class="pull-left m-t-lg">
                    <ol class="hbreadcrumb breadcrumb">
                        <li><a href="index-2.html">پیشخوان</a></li>
                        <li>
                            <span>گزارشات</span>
                        </li>
                        <li class="active">
                            <span>لیست تمام گزارشات</span>
                        </li>
                    </ol>
                </div>
                <h2 class="font-light m-b-xs">
                    گزارشات
                </h2>
                <small>لیست تمام گزارشات
                </small>
            </div>
        </div>
    </div>


    <div class="content">


        <div class="row">
            <div class="col-lg-12">


                <div class="hpanel">
                    <div class="panel-heading">
                        <div class="panel-tools">
                            <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                            <a class="closebox"><i class="fa fa-times"></i></a>
                        </div>
                        جدول گزارشات
                    </div>
                    <div class="panel-body">
                        <div id="example2_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">

                            <div class="row">
                                <div class="col-sm-12">
                                    <table id="example2"
                                        class="table table-striped table-bordered table-hover dataTable no-footer"
                                        role="grid" aria-describedby="example2_info">
                                        <thead>
                                            <tr role="row">
                                                <th class="sorting_asc" tabindex="0" aria-controls="example2"
                                                    rowspan="1" colspan="1" aria-sort="ascending"
                                                    aria-label="محل وقوع حادثه	: activate to sort column descending"
                                                    style="width: 150px;">محل وقوع حادثه	</th>
                                                <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1"
                                                    colspan="1" aria-label="ایستگاه: activate to sort column ascending"
                                                    style="width: 246px;">ایستگاه</th>
                                                <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1"
                                                    colspan="1" aria-label="شیفت: activate to sort column ascending"
                                                    style="width: 150px;">شیفت</th>
                                                <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1"
                                                    colspan="1" aria-label="تعداد مامورین اعزامی	: activate to sort column ascending"
                                                    style="width: 119px;">تعداد مامورین اعزامی	</th>
                                                
                                               
                                                    <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1"
                                                    colspan="1"
                                                    aria-label="شروع تاریخ: activate to sort column ascending"
                                                    style="width: 109px;">جزئیات</th>
                                                  


                                                    <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1"
                                                    colspan="1" aria-label="حقوق: activate to sort column ascending"
                                                    style="width: 81px;">ویرایش</th>

                                                <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1"
                                                    colspan="1" aria-label="یبل: activate to sort column ascending"
                                                    style="width: 81px;">حذف</th>
                                            </tr>
                                        </thead>
                                        <tbody>


                                            <% reportregister.forEach(report => { %>
 
                                                <tr>
                                                  <td>
                              
                                                    <h6 style="color: green;"><%= report.accidentaddress %></h6>
                              
                              
                              
                                                  </td>
                                                  <td>
                              
                                                    <h6 style="color: green;"><%= report.userid.name %></h6>
                              
                              
                                                  </td>
                                                  <td>
                              
                                                    <h6 style="color: green;"><%= report.inputRelatedService %></h6>
                              
                              
                              
                                                  </td>
                      
                                                  <td><%= datas(report.createdAt).format('jD jMMMM jYYYY') %></td>
                                                  <td>
                                                    <%= datas(report.createdAt).fromNow() %>
                                                  </td>

                                                 
                                            <td> <a href="/admin/report/<%= report._id %>/edit"
                                                    class="btn w-xs btn-info btn-xs">ویرایش</a></td>
                                            <td>
                                                
                                                <a href="/admin/report/<%= report._id %>/delete" title="حذف"
                                                    class="btn btn-danger btn-simple btn-xs" onclick="deletes()">
                                                    حذف
                                                </a>
                                                </td>
                                                <td> <a href="/dashboard/report/<%= report.id %>"
                                                    class="btn w-xs btn-primary btn-xs">چاپ</a></td>
                              
                                                </tr>
                              
                              
                                                <% }) %>




                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>

</div>


<!-- DataTables -->
<script src="/vendor/datatables/media/js/jquery.dataTables.min.js"></script>
<script src="/vendor/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
<!-- DataTables buttons scripts -->
<script src="/vendor/pdfmake/build/pdfmake.min.js"></script>
<script src="/vendor/pdfmake/build/vfs_fonts.js"></script>
<script src="/vendor/datatables.net-buttons/js/buttons.html5.min.js"></script>
<script src="/vendor/datatables.net-buttons/js/buttons.print.min.js"></script>
<script src="/vendor/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
<script src="/vendor/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>




<script>
    $(function () {

        // Initialize Example 1
        $('#example2').dataTable({
            dom: "<'row'<'col-sm-4'l><'col-sm-4 text-center'B><'col-sm-4'f>>tp",
            "lengthMenu": [
                [10, 25, 50, -1],
                [10, 25, 50, "All"]
            ],
            buttons: [{
                    extend: 'copy',
                    className: 'btn-sm'
                },

                {
                    extend: 'pdf',
                    title: 'ExampleFile',
                    className: 'btn-sm'
                },
                {
                    extend: 'print',
                    className: 'btn-sm'
                }
            ]
        });

        // Initialize Example 2
        $('#example2').dataTable();

    });
</script>